{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/graphql",
      "dest": "/index.js",
      "methods": ["GET", "POST", "DELETE", "UPDATE"]
    }
  ]
}
